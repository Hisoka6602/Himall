        <!-- type3 富文本 end-->
        <!-- type4 魔方 start-->
        <script type="text/j-template" id="tpl_diy_con_type4">
            <div class="members_con" style="background: <%= content.bgColor %>; padding: <%= content.verticalMargin %>px <%= content.pageMargin %>px">
                <div class="cube-preview" style="margin: -<%= content.imgMargin/2 %>px; height: <%= content.showType < 4 ? content.images[0] && content.images[0].height * (375-content.pageMargin*2)/320 + 'px' : (375-content.pageMargin*2)+'px' %>">
                    <% _.each(content.images,function(item){ %>
                    <% if (item){ %>
                    <div class="item" style="margin: <%= content.imgMargin/2 %>px; width: <%= item.width * (375-content.pageMargin*2)/320 - content.imgMargin + item.width/320 *content.imgMargin %>px; height: <%= item.height * (375-content.pageMargin*2)/320 - content.imgMargin %>px; top: <%= item.top * (375-content.pageMargin*2)/320 %>px; left: <%= item.left * (375-content.pageMargin*2)/320 + item.left/320 *content.imgMargin %>px">
                        <% if (item.imgSrc){ %>
                        <img src="<%= item.imgSrc %>" />
                        <% } %>
                    </div>
                    <% } %>
                    <% }) %>
                </div>
            </div>
        </script>
        <script type="text/j-template" id="tpl_diy_ctrl_type4">
            <div class="modul-hd">魔方</div>
            <% if(content.showType==8){ %>
            <div class="formitems">
                <label class="fi-name">魔方密度：</label>
                <div class="form-controls">
                    <select class="form-control" name="thickness">
                        <option value="4" <% if(content.rowColumn == 4) { %>selected<% } %>>4x4</option>
                        <option value="5" <% if(content.rowColumn == 5) { %>selected<% } %>>5x5</option>
                        <option value="6" <% if(content.rowColumn == 6) { %>selected<% } %>>6x6</option>
                        <option value="7" <% if(content.rowColumn == 7) { %>selected<% } %>>7x7</option>
                    </select>
                </div>
            </div>
            <% } %>
            <div class="form-title">
                <h5>魔方布局</h5>
                <p>选定布局区域，在下方添加图片<%= content.showType > 3 ? '':'，建议添加比例一致的图片' %></p>
            </div>
            <% if(content.showType !== 8) { %>
            <div class="cube-wrap clearfix">
                <% for(var x=0;x<temp.models[content.showType - 1].row;x++){ %>
                    <ul class="cube-row">
                        <% for(var y=0;y
                        <temp.models[content.showType - 1].column;y++){ %>
                            <li style="width: <%= content.showType < 4 ? 320 / content.images.length : 80 %>px; height: <%= content.showType < 4 ? (content.images[0].imgSrc ? content.images[0].height : 320 / content.images.length) : 80 %>px"><%= content.images.length %></li>
                            <% } %>
                    </ul>
                    <% } %>
                    <div>
                        <% _.each(content.images,function(item, index){ %>
                        <div class="cube-item <% if(temp.targetIndex === index) { %>active<% } %>" style="width: <%= temp.models[content.showType - 1].width[index] %>px; height: <%= (content.showType < 4 ? item.height : temp.models[content.showType - 1].height[index]) %>px; left: <%= temp.models[content.showType - 1].left[index] %>px; top: <%= temp.models[content.showType - 1].top[index] %>px;">
                            <% if(item && item.imgSrc) { %>
                            <img src="<%= item.imgSrc %>" />
                            <% }else{ %>
                            <% if(content.showType <4) { %>
                                <p>宽度<%= Math.ceil(750 / content.images.length) %>像素</p>
                                <% }else{ %>
                                <p><%= Math.ceil(temp.models[content.showType - 1].width[index] * 75 / 32) %>x<%= Math.ceil(temp.models[content.showType - 1].height[index] * 75 / 32) %>像素<br />或同等比例</p>
                                <% } %>
                                <% } %>
                        </div>
                        <% }) %>
                    </div>
            </div>
            <% }else{ %>
            <div class="cube-wrap cube-custom clearfix">
                <% for(var x=0;x<content.rowColumn;x++){ %>
                    <ul class="cube-row">
                        <% for(var y=0;y
                        <content.rowColumn;y++){ %>
                            <li class="<% if(temp.cellSelecting.indexOf(x+'_'+y)>-1) { %>selecting<% } %>" data-x="<%= x %>" data-y="<%= y %>" style="width: <%= 320 / content.rowColumn %>px; height: <%= 320 / content.rowColumn %>px;"></li>
                            <% } %>
                    </ul>
                    <% } %>
                    <div>
                        <% _.each(content.images,function(item, index){ %>
                        <div class="cube-item <% if(temp.targetIndex === index) { %>active<% } %>" style="width: <%= item.width %>px; height: <%= item.height %>px; left: <%= item.left %>px; top: <%= item.top %>px;">
                            <% if(item.imgSrc) { %>
                            <img src="<%= item.imgSrc %>" />
                            <% }else{ %>
                            <% if(item.width > 50) { %>
                            <p><%= Math.ceil(item.width * 75 / 32) %>x<%= Math.ceil(item.height * 75 / 32) %><br />或同比例</p>
                            <% }else{ %>
                            <p class="small"><%= Math.ceil(item.width * 75 / 32) %>x<%= Math.ceil(item.height * 75 / 32) %></p>
                            <% } %>
                            <% } %>
                            <i class="cube-remove" data-index="<%= index %>">×</i>
                        </div>
                        <% }) %>
                    </div>
            </div>
            <% } %>

            <% if(temp.targetIndex > -1) { %>
            <ul class="ctrl-item-list" style="margin-left: 30px;">
                <li class="ctrl-item-list-li clearfix">
                    <div class="fl imgnav-box">
                        <div class="imgnav j-selectimg">
                            <% if(content.images[temp.targetIndex].imgSrc && content.images[temp.targetIndex].imgSrc!=""){ %>
                            <img src="<%= content.images[temp.targetIndex].imgSrc %>">
                            <span class="imgnav-reselect">重新选择</span>
                            <% }else{ %>
                            <img src="/Content/Public/images/diy/waitupload.png">
                            <p class="imgnav-reselect">选择图片</p>
                            <% } %>
                        </div>
                        <span class="fi-help-text txtCenter mgt5 j-verify-pic"></span>
                    </div>

                    <div class="fl imgnav-info">
                        <div class="formitems">
                            <label class="fi-name">链接到：</label>
                            <div class="form-controls" style="margin-left: 70px;">
                                <% if(content.images[temp.targetIndex].link.linkType && content.images[temp.targetIndex].link.linkType!=17 && content.images[temp.targetIndex].link.linkType!=23&&content.images[temp.targetIndex].link.linkType!=8){ %>
                                <a href="<%= content.images[temp.targetIndex].link.link %>" target="_blank" class="badge badge-success" title="<%= content.images[temp.targetIndex].link.name %>">
                                    <span><%= HiShop.linkType[content.images[temp.targetIndex].link.linkType] %></span>
                                    <em class="badge-link ovfEps"><%= content.images[temp.targetIndex].link.name %></em>
                                </a>
                                <%}%>

                                <% if(content.images[temp.targetIndex].link.linkType==17|| content.images[temp.targetIndex].link.linkType==23|| content.images[temp.targetIndex].link.linkType==8){ %>
                                <input type="text" name="customlink" class="input  input-no-at" value="<%= content.images[temp.targetIndex].link.link %>">
                                <% } %>

                                <div class="droplist">
                                    <a href="javascript:;" class="droplist-title j-droplist-toggle">
                                        <% if(content.images[temp.targetIndex].link.linkType==0){ %>
                                        <span>请选择</span>
                                        <%}else{%>
                                        <span>修改</span>
                                        <%}%>
                                        <i class="gicon-chevron-down mgl5"></i>
                                    </a>
                                    <ul class="droplist-menu">
                                        <li data-val="1"><a href="javascript:;"><%= HiShop.linkType[1] %></a></li>
                                        <li data-val="2"><a href="javascript:;"><%= HiShop.linkType[2] %></a></li>
                                        <li data-val="5"><a href="javascript:;"><%= HiShop.linkType[5] %></a></li>
                                        <li data-val="6"><a href="javascript:;"><%= HiShop.linkType[6] %></a></li>
                                        <li data-val="23"><a href="javascript:;"><%= HiShop.linkType[23] %></a></li>
                                        <li data-val="3"><a href="javascript:;"><%= HiShop.linkType[3] %></a></li>
                                        <li data-val="8"><a href="javascript:;"><%= HiShop.linkType[8] %></a></li>
                                        <li data-val="9"><a href="javascript:;"><%= HiShop.linkType[9] %></a></li>
                                        <li data-val="32"><a href="javascript:;"><%= HiShop.linkType[32] %></a></li>
                                        <li data-val="40"><a href="javascript:;"><%= HiShop.linkType[40] %></a></li>
                                    </ul>
                                </div>
                                <% if(content.images[temp.targetIndex].link.linkType==8){ %>
                                <span class="fi-help-text">外链请加上 http:// 前缀</span>
                                <% } %>
                                <% if(content.images[temp.targetIndex].link.linkType==17){ %>
                                <span class="fi-help-text">请输入小程序AppID</span>
                                <% } %>
                                <% if(content.images[temp.targetIndex].link.linkType==23){ %>
                                <span class="fi-help-text">输入格式：电话号码</span>
                                <% } %>
                                <input type="hidden" class="j-verify" name="item_id" value="">
                                <span class="fi-help-text j-verify-linkType"></span>
                            </div>
                        </div>
                    </div>
                </li>
            </ul>
            <% } %>

            <div class="formitems">
                <label class="fi-name">选择模板：</label>
                <div class="form-controls">
                    <ul class="models-btn">
                        <% _.each(temp.models,function(item){ %>
                        <li class="<% if(content.showType === item.value){ %>active<% } %>">
                            <i class="iconfont icon-cube-<%= item.value %>"></i>
                            <div class="tips">
                                <%= item.name %>
                                <div class="arrow"></div>
                            </div>
                        </li>
                        <% }) %>
                    </ul>
                </div>
            </div>
            <div class="formitems">
                <label class="fi-name">背景颜色：</label>
                <div class="form-controls">
                    <div class="color-select">
                        <span style="background-color:<%=content.bgColor%>"></span>
                    </div>
                </div>
            </div>
            <div class="formitems">
                <label class="fi-name">页面边距：</label>
                <div class="form-controls">
                    <div class="slider pageMargin"></div>
                    <span class="fl mgl10 j-ctrl-showheight lh28"><%= content.pageMargin %>像素</span>
                </div>
            </div>
            <div class="formitems">
                <label class="fi-name">上下边距：</label>
                <div class="form-controls">
                    <div class="slider verticalMargin"></div>
                    <span class="fl mgl10 j-ctrl-showheight lh28"><%= content.verticalMargin %>像素</span>
                </div>
            </div>
            <div class="formitems">
                <label class="fi-name">图片间距：</label>
                <div class="form-controls">
                    <div class="slider imgMargin"></div>
                    <span class="fl mgl10 j-ctrl-showheight lh28"><%= content.imgMargin %>像素</span>
                </div>
            </div>

        </script>
        <!-- type4 魔方 end-->
        <!-- type5 文字链接 start-->
