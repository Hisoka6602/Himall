        <script type="text/j-template" id="tpl_diy_con_type8">
            <div class="component-preview" style="background-color:<%= content.bgColor %>; padding:<%= content.verticalMargin %>px 0; border-radius: <%= content.moduleRadius?'8px':0 %>">
                <% if(!content.goodslist.length){ %>
                <div class="default-preview">
                    <h3>点击添加商品</h3>
                </div>
                <% } %>
                <ul class="product-list style<%=content.showType%> <% if(content.listStyle==6) { %>list-waterfall<% } %>" style="margin: -<%= content.productMargin/2 %>px; padding: 0 <%= content.pageMargin %>px">
                    <% _.each(content.goodslist,function(item, index){ %>
                    <li style="<% if(content.fontStyle) { %>font-weight:bold<% } %>">
                        <div class="product-wrap" style="margin: <%= content.pageMargin/2 %>px; border-radius: <%= content.radius ? '8px':0 %>; background-color: <%= content.listStyle==4 ?'transparent':'#fff' %>; box-shadow: <%= content.listStyle==2 ?'0 2px 8px rgba(93,113,127,.08)':'none' %>; border: <%= content.listStyle==3 ? '1px solid #f2f2f2':'0' %> ">
                            <% if(content.listStyle!=6){ %>
                            <div class="product-img" style="padding-top:<%=content.imgRatio * 100 %>%">
                                <img class="el-image" src="<%= item.pic %>" style="object-fit:<%= content.imgFill ? 'cover': 'scale-down' %> " />
                            </div>
                            <% }else{ %>
                            <div class="product-img">
                                <img class="el-image" src="<%= item.pic %>" style="object-fit:scale-down" />
                            </div>
                            <% } %>
                            <% if(content.showTag){ %>
                            <div class="tag">
                                <% if(content.tagStyle<5){ %>
                                    <img class="el-image" src="/Content/Public/images/diy/tag<%= content.tagStyle %>.png" />
                                    <% }else{ %>
                                    <% if(content.tagImg){ %>
                                    <img class="el-image" src="<%= content.tagImg %>" />
                                    <% }else{ %>
                                    <div class="tag-area">角标<br />区域</div>
                                    <% } %>
                                    <% } %>
                            </div>
                            <% } %>
                            <div class="product-info" style="<% if(content.textAlign) { %>text-align:center<% } %>">
                                <% if(content.showName){ %>
                                <h3 style="color:<%= content.nameColor %>" class="<% if(content.nameLine==2) { %>two-line<% } %>">
                                    <% if(content.showNameIcon){ %>
                                    <% if(content.nameIcon){ %>
                                    <img class="el-image" src="<%= content.nameIcon %>" />
                                    <% }else{ %>
                                    <div class="image-slot">图标区域</div>
                                    <% } %>
                                    <% } %>
                                    <%= item.title %>
                                </h3>
                                <% } %>
                                <% if(content.showDesc){ %>
                                <p style="color: <%= content.descColor %>"><%= item.desc %></p>
                                <% } %>
                                <% if(content.showPrice || content.showBtn){ %>
                                <div class="buy-info" class="<% if(content.textAlign) { %>flex-column<% } %>">
                                    <% if(content.showPrice){ %>
                                    <span class="price" style="color:<%= content.priceColor %>"><i>¥</i><%= item.price %></span>
                                    <% } %>
                                    <% if(content.showBtn){ %>
                                    <% if(content.listStyle==5){ %>
                                    <span class="btn-limit">我要抢购</span>
                                    <% }else if(content.btnStyle!=9){ %>
                                    <span class="iconbtn iconfont style<%= content.btnStyle %> <% if(content.btnStyle==1) { %>icon-cart2<% } %> <% if(content.btnStyle==2) { %>icon-cart1<% } %> <% if(content.btnStyle==3) { %>icon-plus-empty<% } %>  <% if(content.btnStyle==4) { %>icon-plus-fill<% } %>"><%= content.btnStyle>4? content.btnText :'' %></span>
                                    <% }else { %>
                                    <% if(content.btnImg){ %>
                                    <img class="el-image" src="<%= content.btnImg %>" style="object-fit:scale-down" />
                                    <% }else { %>
                                    <div class="image-slot">按钮区域</div>
                                    <% } %>
                                    <% } %>
                                    <% } %>
                                </div>
                                <% } %>
                            </div>
                        </div>
                    </li>
                    <% }) %>
                </ul>
            </div>
        </script>



