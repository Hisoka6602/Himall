        <script type="text/j-template" id="tpl_diy_con_type15">
            <div class="component-preview" style="background-color:<%= content.bgColor %>; padding:<%= content.verticalMargin %>px 0; border-radius: <%= content.moduleRadius?'8px':0 %>">
                <% if(content.goodslist && content.goodslist.length==0){ %>
                <div class="default-preview">
                    <h3>点击选择拼团商品</h3>
                </div>
                <% } %>
                <ul class="product-list group-product-view style<%=content.showType%> <% if(content.listStyle==6) { %>list-waterfall<% } %>" style="margin: -<%= content.productMargin/2 %>px; padding: 0 <%= content.pageMargin %>px">
                    <% _.each(content.goodslist,function(item, index){ %>
                    <li style="<% if(content.fontStyle) { %>font-weight:bold<% } %>">
                        <div class="product-wrap" style="margin: <%= content.productMargin/2 %>px; border-radius: <%= content.radius ? '8px':0 %>; background-color: <%= content.listStyle==4 ?'transparent':'#fff' %>; box-shadow: <%= content.listStyle==2 ?'0 2px 8px rgba(93,113,127,.08)':'none' %>; border: <%= content.listStyle==3 ? '1px solid #f2f2f2':'0' %> ">
                            <% if(content.listStyle!=6){ %>
                            <a class="product-img" style="padding-top:<%=content.imgRatio * 100 %>%" href="<%= item.link %>">
                                <img class="el-image" src="<%= item.pic %>" style="object-fit:<%= content.imgFill ? 'cover': 'scale-down' %> " />
                                <% if(content.showTime && content.showType!==3 && content.showType!==6){ %>
                                <div class="time <%= content.showType === 1 || (content.showType === 5 && index%3===0) ? 'big':'' %>">
                                    <% if(content.showType === 1 || (content.showType === 5 && index%3===0)){ %>
                                    <span>
                                        <%= item.beginSec>0?'距开始':'距结束' %>
                                        仅剩 <br />
                                        <% if(item.timeText.day){ %>
                                        <i><%= item.timeText.day %></i>天<% } %><i><%= item.timeText.hour %></i>时<i><%= item.timeText.minute %></i>分<i><%= item.timeText.second %></i>
                                    </span>
                                    <% }else{ %>
                                    <%= item.beginSec>0?'距开始':'距结束' %>
                                    <span>
                                        <% if(item.timeText.day){ %>
                                        <i><%= item.timeText.day %></i>天<% } %><i><%= item.timeText.hour %></i>:<i><%= item.timeText.minute %></i>:<i><%= item.timeText.second %></i>
                                    </span>
                                    <% } %>
                                </div>
                                <% } %>
                            </a>
                            <% }else{ %>
                            <a class="product-img" href="<%= item.link %>">
                                <img class="el-image" src="<%= item.pic %>" style="object-fit:scale-down" />
                                <% if(content.showTime && content.showType!==3 && content.showType!==6){ %>
                                <div class="time">
                                    <%= item.beginSec>0?'距开始':'距结束' %>
                                    <span>
                                        <% if(item.timeText.day){ %>
                                        <i><%= item.timeText.day %></i>天<% } %><i><%= item.timeText.hour %></i>:<i><%= item.timeText.minute %></i>:<i><%= item.timeText.second %></i>
                                    </span>
                                </div>
                                <% } %>
                            </a>
                            <% } %>
                            <div class="product-info" style="<% if(content.textAlign) { %>text-align:center<% } %>">
                                <% if(content.showName){ %>
                                <h3 style="color:<%= content.nameColor %>" class="<% if(content.nameLine==2) { %>two-line<% } %>">
                                    <% if(content.showNameIcon){ %>
                                    <% if(content.nameIcon){ %>
                                    <img class="el-image" src="<%= content.nameIcon %>" />
                                    <% }else{ %>
                                    <div class="image-slot">图标区域</div>
                                    <% } %>
                                    <% } %>
                                    <span class="limit-tag"><%= item.number %>人团</span>
                                    <em><%= item.title %></em>
                                </h3>
                                <% } %>
                                <% if((content.showProcess && content.showType!==3 && content.showType!==6) || content.showStock){ %>
                                <div class="sale-count">
                                    <% if(content.showProcess && content.showType!==3 && content.showType!==6){ %>
                                    <div class="el-progress-bar__outer">
                                        <div class="el-progress-bar__inner" style="width: <%= item.percentage %>%;"></div>
                                    </div>
                                    <% } %>
                                    <% if(content.showProcess || content.showStock){ %>
                                    <span class="stock" style="opacity:<%= content.showStock?1:0 %>">仅剩<i><%= item.stock %></i>件</span>
                                    <% } %>
                                    <% if(content.showProcess && content.showType!==3 && content.showType!==6){ %>
                                    <span class="sale">已抢<i><%= item.count %></i>件</span>
                                    <% } %>
                                </div>
                                <% } %>
                                <% if(content.showPrice || content.showBtn){ %>
                                <div class="buy-info <% if(content.textAlign==1) { %>flex-column<% } %>">
                                    <span class="price">
                                        <% if(content.showPrice){ %>
                                        <span style="color:<%= content.priceColor %>"><i>¥</i><%= item.price %></span>
                                        <% } %>
                                        <% if(content.showOldPrice){ %>
                                        <s>¥<%= item.saleprice %></s>
                                        <% } %>
                                    </span>
                                    <% if(content.showBtn){ %>
                                    <% if(content.listStyle==5){ %>
                                    <span data-href="<%= item.link %>" data-activitytype='2' class="btn-limit">我要抢购</span>
                                    <% }else if(content.btnStyle!=9){ %>
                                    <span data-href="<%= item.link %>" data-activitytype='2' class="iconbtn iconfont style<%= content.btnStyle %> <% if(content.btnStyle==1) { %>icon-cart2<% } %> <% if(content.btnStyle==2) { %>icon-cart1<% } %> <% if(content.btnStyle==3) { %>icon-plus-empty<% } %>  <% if(content.btnStyle==4) { %>icon-plus-fill<% } %>"><%= content.btnStyle>4? content.btnText :'' %></span>
                                    <% }else { %>
                                    <% if(content.btnImg){ %>
                                    <img data-href="<%= item.link %>" data-activitytype='2' class="el-image" src="<%= content.btnImg %>" style="object-fit:scale-down" />
                                    <% }else { %>
                                    <div class="image-slot">按钮区域</div>
                                    <% } %>
                                    <% } %>
                                    <% } %>
                                </div>
                                <% } %>
                            </div>
                        </div>
                    </li>
                    <% }) %>
                </ul>
            </div>
        </script>

