            <div class="formitems">
                <label class="fi-name">边距：</label>
                <div class="form-controls">
                    <div class="radio-group">
                        <label><input type="radio" name="margin" value="0"  <% if(content.margin=='0'){ %>checked<% } %> /> 无边距</label>
                        <label><input type="radio" name="margin" value="12"  <% if(content.margin=='12'){ %>checked<% } %> /> 左右留边</label>
