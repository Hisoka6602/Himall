                        <label><input type="radio" name="showType" value="2" <% if(content.showType==2) { %>checked<% } %>>一行两个</label>
                        <label><input type="radio" name="showType" value="3" <% if(content.showType==3) { %>checked<% } %>>一行三个</label>
