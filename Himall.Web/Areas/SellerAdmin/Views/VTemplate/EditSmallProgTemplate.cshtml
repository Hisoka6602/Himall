                        <label><input type="radio" name="showType" value="5" <% if(content.showType==5) { %>checked<% } %>>一大两小</label>
                        <label><input type="radio" name="showType" value="6" <% if(content.showType==6) { %>checked<% } %>>横向滑动</label>
