                <label class="fi-name">一屏显示：</label>
                <div class="form-controls">
                    <select class="form-control" name="thickness">
                        <option value="4" <% if(content.lineCount == 4) { %>selected<% } %>>4个导航</option>
                        <option value="5" <% if(content.lineCount == 5) { %>selected<% } %>>5个导航</option>
                        <option value="6" <% if(content.lineCount == 6) { %>selected<% } %>>6个导航</option>
                        <option value="7" <% if(content.lineCount == 7) { %>selected<% } %>>7个导航</option>
                        <option value="8" <% if(content.lineCount == 8) { %>selected<% } %>>8个导航</option>
                        <option value="9" <% if(content.lineCount == 9) { %>selected<% } %>>9个导航</option>
                        <option value="10" <% if(content.lineCount == 10) { %>selected<% } %>>10个导航</option>
                    </select>
