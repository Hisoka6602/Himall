    <script>
        //拖拽
        function dragAnimate(obj) {
            var ispeed = 200;
            obj.animate({
                left: $(window).width() - obj.width()
            }, ispeed);
        }
        $(function () {
            var oWin = $(window);
            var disX = 0;
            var disY = 0
            var oMargin = $('.diy-actions').height() / 2;
            //var oMargin = Math.abs(parseInt( $('.diy-actions').outerHeight(true) - $('.diy-actions').outerHeight() ));
            //console.log(oMargin);

            $('.diy-actions-drag').mousedown(function (ev) {
                disX = ev.pageX - $(this).offset().left;
                disY = ev.pageY - $(this).offset().top;
                $('.diy-actions').addClass('diy-actions-shadow');
                $('html').mousemove(function (ev) {
                    var scrollTop = oWin.scrollTop();
                    var t = ev.pageY - disY - scrollTop + oMargin;
                    var l = ev.pageX - disX - $('.diy-actions-fold').width();
                    var w = oWin.width() - $('.diy-actions').width();
                    var h = oWin.height() - $('.diy-actions').height() + oMargin;
                    if (t < oMargin) {
                        t = oMargin;
                    }
                    else if (t > h) {
                        t = h;
                    }
                    if (l < 0) {
                        l = 0;
                    }
                    else if (l > w) {
                        l = w;
                    }
                    $('.diy-actions').css({
                        'top': t,
                        'left': l,
                    });
                });
                $('html').mouseup(function () {
                    $('html').off();
                    $('.diy-actions').removeClass('diy-actions-shadow');
                    if (oWin.width() - $('.diy-actions').width() - $('.diy-actions').offset().left <= 80) {
                        dragAnimate($('.diy-actions'));
                    }
                });
                return false;
            });


            //点击切换到右边
            $('.diy-actions-fold').on('click', function () {
                dragAnimate($('.diy-actions'));
            });
        });
    </script>
