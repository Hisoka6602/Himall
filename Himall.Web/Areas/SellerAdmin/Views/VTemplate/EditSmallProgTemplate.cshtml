                        <label><input type="radio" name="moduleRadius" value="1" <% if(content.moduleRadius) { %>checked<% } %>>圆角</label>
                    </div>
                </div>
            </div>
            <div class="formitems">
                <label class="fi-name">背景颜色：</label>
                <div class="form-controls">
                    <div class="color-select bgColor">
                        <span style="background-color:<%=content.bgColor%>"></span>
                    </div>
                </div>
            </div>
            <div class="formitems">
                <label class="fi-name">文字颜色：</label>
                <div class="form-controls">
                    <div class="color-select textColor">
                        <span style="background-color:<%=content.textColor%>"></span>
                    </div>
                </div>
            </div>

