@{
    Layout = null;
}
<!doctype html>
<html lang="zh-CN">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>用户注册协议</title>
    <link rel="stylesheet" type="text/css" href="~/Content/bootstrap.min.css" />
