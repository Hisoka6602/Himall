            <div class="form-group">
                <div class="col-sm-10 col-sm-offset-1" id="divhtml">@Html.Raw(Model.AgreementContent)</div>
            </div>
            @*<div class="form-group">
                    <div class="col-sm-offset-1 col-sm-4">
                        <label><input type="checkbox" id="agree" name="agree" /> </label>
                    </div>
                </div>*@

            <div class="form-group">
                <div class="col-sm-offset-1 col-sm-10">
                    <button type="button" class="btn btn-primary" onclick="window.close()">我已阅读并同意以上协议</button>
                </div>
            </div>
        </form>
    </div>
